prompt(“Are you ready to play?”);
if (prompt = “yes”) {
	console.log(“Go ahead and play”);
} else {
	console.log(“You're no fun”);
}
var Age = prompt(“What is your age?”);
if (Age < 18) {
	console.log(“You may not play”);
} else {
	console.log(“You may play”);
}
	console.log(“Your name is Justin Bieber and you were arrested for malicious acts”);
	prompt(“You see a set of keys shoved into the wall under your cot, do you take them?”);
if (prompt = “yes”) {
	console.log(“The keys are stuck, and the guard sees you doing something suspicious, your sentence has been extended to 10,000 years”);
} else {
	console.log(“Wise choice”);
}
if (console.log(“ The keys are stuck, and the guard sees you doing something suspicious, your sentence has been extended to 10,000 years”) {
	console.log(“You slowly rot away in prison until the day you die! Game End.”);
} else {
	console.log(“You notice that the guard left the room and there's a small crowbar resting under the cot as well. Why? Beats me”);
}
prompt(“Do you take the crowbar and wedge it under the keys?”);
if (prompt = “yes”) {
	console.log(“The guard returns and you have been caught, Game End.”);
} else {
	console.log(“Patience is a virtue they say”);
}
	prompt(“Do you serve the remainder of your sentence?”);
if (prompt = “yes) {
	console.log(“Congratulations! You win, or something”);
} else {
	console.log(“You kinda have no choice here.... Game End.”);
}
	prompt(“Rate my game out of 10”);
if (prompt < 8) {
	console.log(“How mean!”);
} else {
	console.log(“Thanks for playing”);
}
